@using BOL;
@using System.Collections.Generic;
@{
    List<BOL.Product> products = (List<BOL.Product>)ViewData["catalog"]; 
    <ol>
        @{
            foreach(Product product in products)
            {
                <li>@product.Pid @product.Pname @product.Qty @product.Price</li>

            }
        }
    </ol>
}
